    "build:vault-server": "turbo build --filter=vault-server"
  },
  "devDependencies": {
    "@ledgerhq/eslint-config-custom": "workspace:*",
    "@types/node": "^18.15.13",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}